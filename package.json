{
  "name": "texei-sfdx-plugin",
  "description": "Texe誰's plugin for sfdx",
  "version": "2.0.1",
  "author": "Texe誰",
  "bugs": "https://github.com/texei/texei-sfdx-plugin/issues",
  "dependencies": {
    "@oclif/core": "^2.1.1",
    "@salesforce/core": "^3.33.0",
    "@salesforce/kit": "^1.8.3",
    "@salesforce/packaging": "^1.2.7",
    "@salesforce/sf-plugins-core": "^2.0.1",
    "@types/child-process-promise": "^2.2.2",
    "@types/jsforce": "^1.11.0",
    "@types/xml2js": "^0.4.11",
    "chalk": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "csvtojson": "^2.0.10",
    "puppeteer": "^21.0.3",
    "tslib": "^2",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.2.18",
    "@oclif/plugin-version": "^1.2.0",
    "@salesforce/cli-plugins-testkit": "^3.2.20",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.1",
    "@salesforce/plugin-auth": "^2.3.12",
    "@salesforce/plugin-command-reference": "^2.2.9",
    "@salesforce/plugin-config": "^1.4.23",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.4.5",
    "@salesforce/ts-types": "^1.5.20",
    "@swc/core": "^1.3.35",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chai": "^4.3.7",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-sf-plugin": "^1.8.1",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.6.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "^11.1.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.9.4"
  },
  "config": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas"
  ],
  "homepage": "https://github.com/texei/texei-sfdx-plugin",
  "keywords": [
    "texe誰",
    "texei-sfdx-plugin",
    "sfdx",
    "sfdx-plugin",
    "salesforcedx"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference",
      "@salesforce/plugin-config",
      "@salesforce/plugin-auth"
    ],
    "plugins": [],
    "topics": {
      "texei": {
        "description": "Texe誰's plugin for sfdx",
        "subtopics": {
          "org": {
            "description": "Commands to manage org"
          },
          "data": {
            "description": "Commands to manage data"
          }
        }
      }
    }
  },
  "repository": "https://github.com/texei/texei-sfdx-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 1800000 --parallel",
    "test:nuts:package": "nyc mocha \"test/commands/package/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "test:nuts:package1": "nyc mocha \"test/commands/package1/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  }
}
